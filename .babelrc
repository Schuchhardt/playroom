{
    "presets": [
      ["env", {
        "modules": false,
        "targets": {
          "browsers": "> 1%",
          "uglify": true
        },
        "useBuiltIns": true
      }]
    ],
  
    "plugins": [
      "@babel/syntax-dynamic-import",
      "transform-object-rest-spread",
      ["transform-class-properties", { "spec": true }]
    ]
  }
  